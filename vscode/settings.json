{
  "editor.fontFamily": "'Fira Code'",
  "editor.cursorBlinking": "solid",
  "editor.fontLigatures": true,
  "editor.wordWrap": "on",
  "editor.fontSize": 13,
  "editor.fontWeight": "500",
  "editor.acceptSuggestionOnEnter": "off",
  "gitlens.codeLens.enabled": false,
  "go.lintTool": "revive",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.mypyPath": "/usr/bin/mypy",
  "python.formatting.blackPath": "/usr/bin/black",
  "python.linting.mypyEnabled": true,
  "python.formatting.provider": "black",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.associations": {
    "*.groovy.template": "groovy",
    "Jenkinsfile*": "groovy",
    "*.Jenkinsfile": "groovy",
    "*.hcl": "terraform"
  },
  "search.exclude": {
    "**/.mypy_cache": true
  },
  "editor.cursorSurroundingLines": 3,
  "[python]": {
    "editor.rulers": [
      88
    ]
  },
  "[go]": {
    "editor.rulers": [
      100
    ]
  },
  "[plaintext]": {
    "editor.rulers": [
      100
    ]
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[terraform]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "editor.multiCursorModifier": "ctrlCmd",
  "go.useLanguageServer": true,
  "terminal.integrated.fontFamily": "'Fira Code'",
  "explorer.confirmDragAndDrop": false,
  "tabnine.experimentalAutoImports": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Ayu Dark",
  "editor.minimap.enabled": false,
  "editor.suggest.showStatusBar": true,
  "python.experiments.enabled": false,
  "python.autoComplete.extraPaths": [
    "./.venv"
  ],
  "go.toolsManagement.autoUpdate": true,
  "files.trimTrailingWhitespace": true,
  "redhat.telemetry.enabled": false,
  "git.enableStatusBarSync": false,
  "terminal.integrated.tabs.enabled": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "python.terminal.activateEnvironment": false,
  "workbench.iconTheme": "ayu",
  "pythonTestExplorer.testFramework": "pytest",
  "editor.suggest.preview": true,
  "extensions.autoUpdate": false,
  "files.exclude": {
    "**/.mypy_cache": true,
    "**/.pytest_cache": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "thunder-client.codeSnippetLanguage": "python-requests",
  "editor.formatOnSave": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "python.linting.pylintArgs": [
    "--disable=missing-function-docstring",
    "--disable=missing-class-docstring",
    "--disable=missing-module-docstring"
  ],
  "git-graph.commitDetailsView.location": "Docked to Bottom",
  "editor.guides.bracketPairs": true,
  "diffEditor.renderSideBySide": false,
  "rest-client.enableTelemetry": false,
  "css.validate": false,
  "deno.enable": true,
  "rest-client.previewOption": "exchange",
  "svelte.enable-ts-plugin": true,
  "telemetry.telemetryLevel": "off",
  "python.languageServer": "Pylance",
  "terraform.codelens.referenceCount": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "git.mergeEditor": true,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "elixirLS.enableTestLenses": true,
  "elixirLS.suggestSpecs": false,
  "window.zoomLevel": -1,
  "window.menuBarVisibility": "toggle",
  "window.titleBarStyle": "custom"
}
